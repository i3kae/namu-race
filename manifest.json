{
  "name": "namu race",
  "description": "namu wiki race game",
  "version": "0.1",
  "manifest_version": 3,
  "action": {
    "default_popup": "main.html",
    "default_icon": "images/icon.png"
  },
  "permissions": ["tabs"],
  "content_scripts": [
    {
      "matches": ["https://namu.wiki/w/*"],
      "js": ["src/gamePlay.js"]
    }
  ]
}
